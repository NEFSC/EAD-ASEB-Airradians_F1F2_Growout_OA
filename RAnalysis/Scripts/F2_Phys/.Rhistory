F2_DryShell <- F2_dryweights %>%
dplyr::select(Age, pCO2, Tank_Replicate, Dry_Shell_weight_g, Shell_length_mm) %>%
na.omit()
# call the mean Shell dry weight
meanLength.all <- mean(F2_DryShell$Shell_length_mm) # 33.58173
seLength.all   <- sd(F2_DryShell$Shell_length_mm)/sqrt(length((F2_DryShell$Shell_length_mm))) # 0.4933574
# age range of animals
print(paste0('youngest = ', min(F2_DryShell$Age), '; oldest = ', max(F2_DryShell$Age)))
#assign bfactors
bLength.all  <- 2.95#
bLength.LvMall  <- 2.92
bLength.LvHall  <- 2.96
bLength.low  <- 2.9#
bLength.mod  <- 2.93 #3
bLength.high <- 3.01#
# standardize Shell dry weight using bfactors
F2_DryShell.bfactor <- F2_DryShell %>%
dplyr::mutate(
Shell_bFactorLength.all  =  ( (Dry_Shell_weight_g * (Shell_length_mm^-bLength.all))* # a
(meanLength.all^bLength.all) ) * 100,
Shell_bFactorLength.pCO2 =  case_when(pCO2 == '500 μatm' ~
( (Dry_Shell_weight_g * (Shell_length_mm^-bLength.low))* # a
(meanLength.all^bLength.low) ) * 100,
pCO2 == '800 μatm' ~
( (Dry_Shell_weight_g * (Shell_length_mm^-bLength.mod))* # a
(meanLength.all^bLength.mod) ) * 100,
pCO2 == '1200 μatm' ~
( (Dry_Shell_weight_g * (Shell_length_mm^-bLength.high))* # a
(meanLength.all^bLength.high) ) * 100
) # case when for pCO2
) # mutate
# pivot longer and plot
F2_DryShell.bfactor.long <- F2_DryShell.bfactor %>%
dplyr::select(-Shell_length_mm) %>%
tidyr::pivot_longer(
cols = c(Dry_Shell_weight_g, Shell_bFactorLength.all, Shell_bFactorLength.pCO2),
names_to = "measurement",
values_to = "value"
)
